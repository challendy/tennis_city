%h2 Sign up

- form_for resource_name, resource, :url => registration_path(resource_name) do |f|
  = f.error_messages
  %fieldset
    %ol
      %li
        = f.label :email
        = f.text_field :email
      %li
        =f.label :password
        =f.password_field :password
      %li
        =f.label :password_confirmation
        =f.password_field :password_confirmation

      %li
        =f.submit "Sign up"

= render :partial => "shared/devise_links"
